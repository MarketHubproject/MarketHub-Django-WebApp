  "regex_patterns": {
    "word_boundary_patterns": [
      {
        "pattern": "\\b[Ss]tore[ _-]?[Ll]ite\\b",
        "replacement": "MarketHub",
        "description": "Matches Store Lite with optional separators, case insensitive start"
      },
      {
        "pattern": "\\b[Ss][Tt][Oo][Rr][Ee][ _-]?[Ll][Ii][Tt][Ee]\\b",
        "replacement": "MarketHub",
        "description": "Case insensitive Store Lite with optional separators"
      },
      {
        "pattern": "\\bSTORE[ _-]?LITE\\b",
        "replacement": "MARKETHUB",
        "description": "All caps Store Lite with optional separators"
      },
      {
        "pattern": "\\bstore[ _-]?lite\\b",
        "replacement": "markethub",
        "description": "All lowercase store lite with optional separators"
      }
    ],
    "camelcase_patterns": [
      {
        "pattern": "\\bstoreLite\\b",
        "replacement": "marketHub",
        "description": "CamelCase starting with lowercase"
      },
      {
        "pattern": "\\bStoreLite\\b",
        "replacement": "MarketHub", 
        "description": "PascalCase StoreLite"
      }
    ],
    "file_path_patterns": [
      {
        "pattern": "([/\\\\]|^)store[_-]?lite([/\\\\]|\\.)",
        "replacement": "$1markethub$2",
        "description": "File paths containing store lite"
      },
      {
        "pattern": "([/\\\\]|^)Store[_-]?Lite([/\\\\]|\\.)",
        "replacement": "$1MarketHub$2",
        "description": "File paths containing Store Lite"
      },
      {
        "pattern": "([/\\\\]|^)STORE[_-]?LITE([/\\\\]|\\.)",
        "replacement": "$1MARKETHUB$2",
        "description": "File paths containing STORE LITE"
      }
    ],
    "css_scss_patterns": [
      {
        "pattern": "\\$store[_-]?lite[_-]",
        "replacement": "$markethub-",
        "description": "SCSS variables starting with store lite"
      },
      {
        "pattern": "\\$Store[_-]?Lite[_-]",
        "replacement": "$MarketHub-",
        "description": "SCSS variables starting with Store Lite"
      },
      {
        "pattern": "\\$STORE[_-]?LITE[_-]",
        "replacement": "$MARKETHUB-",
        "description": "SCSS variables starting with STORE LITE"
      },
      {
        "pattern": "--store[_-]?lite[_-]",
        "replacement": "--markethub-",
        "description": "CSS custom properties with store lite"
      },
      {
        "pattern": "--Store[_-]?Lite[_-]",
        "replacement": "--MarketHub-",
        "description": "CSS custom properties with Store Lite"
      }
    ],
    "html_attribute_patterns": [
      {
        "pattern": "class=[\"']([^\"']*?)store[_-]?lite([^\"']*?)[\"']",
        "replacement": "class=\"$1markethub$2\"",
        "description": "HTML class attributes containing store lite"
      },
      {
        "pattern": "id=[\"']([^\"']*?)store[_-]?lite([^\"']*?)[\"']",
        "replacement": "id=\"$1markethub$2\"",
        "description": "HTML id attributes containing store lite"
      }
    ],
    "javascript_patterns": [
      {
        "pattern": "\\bstore[_-]?lite([A-Z])",
        "replacement": "markethub$1",
        "description": "JavaScript camelCase variables starting with storeLite"
      },
      {
        "pattern": "\\bStore[_-]?Lite([A-Z])",
        "replacement": "MarketHub$1",  
        "description": "JavaScript PascalCase variables starting with StoreLite"
      }
    ]
  },
