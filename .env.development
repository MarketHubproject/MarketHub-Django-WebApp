# Development Environment Configuration
NODE_ENV=development

# Backend API
API_BASE_URL=https://dev-api.markethub.com
TIMEOUT_MS=10000

# Firebase Configuration
FIREBASE_PROJECT_ID=markethub-dev
FIREBASE_APP_ID=1:123456789:ios:dev
GOOGLE_SERVICES_JSON=google-services-dev.json

# Stream Chat
STREAM_CHAT_API_KEY=dev-stream-key-12345
SUPPORT_EMAIL=dev-support@markethub.com
CHAT_BOT_USER_ID=dev-markethub-bot
CHAT_BOT_NAME=DevBot Assistant
CHAT_SUPPORT_AGENT_ID=dev-support-agent

# Feature Flags (Development)
ENABLE_CHAT_SUPPORT=true
ENABLE_FAQ_SEARCH=true
ENABLE_LIVE_CHAT=true
ENABLE_AR_FEATURES=true
ENABLE_SOCIAL_SHARING=true
ENABLE_REWARDS_PROGRAM=true
ENABLE_OFFLINE_MODE=true
ENABLE_ANALYTICS=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_DEBUG_MODE=true

# Analytics
ANALYTICS_TRACKING_ID=dev-analytics-123
ENABLE_ANALYTICS_DEBUG=true

# Branch.io
BRANCH_KEY=key_test_dev123
BRANCH_SECRET=secret_dev456

# Sentry
SENTRY_DSN=https://dev@sentry.io/markethub
SENTRY_ENVIRONMENT=development

# Performance
ENABLE_PERFORMANCE_MONITORING=true
PERFORMANCE_SAMPLE_RATE=1.0

# Debugging
FLIPPER_ENABLED=true
REACTOTRON_ENABLED=true
